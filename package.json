{
	"name": "inventory",
	"description": "Nextcloud Inventory",
	"version": "0.0.1",
	"author": {
		"name": "Raimund Schlüßler",
		"email": "raimund.schluessler@mailbox.org"
	},
	"license": "AGPLv3",
	"private": true,
	"homepage": "https://gitlab.rschluessler.com/raimund.schluessler/Inventory",
	"scripts": {
		"dev": "npm run sprites && webpack --config webpack.dev.js",
		"watch": "webpack --progress --watch --config webpack.dev.js",
		"build": "npm run sprites && webpack --progress --hide-modules --config webpack.prod.js",
		"lint": "eslint --ext .js,.vue src tests",
		"lint:fix": "eslint --ext .js,.vue src tests --fix",
		"test": "jest --verbose",
		"sprites": "svg-sprite --config svg-sprite-color.json img/src/color/*.svg && svg-sprite --config svg-sprite-bw.json img/src/bw/*.svg"
	},
	"repository": {
		"type": "git",
		"url": "git@gitlab.rschluessler.com:raimund.schluessler/Inventory.git"
	},
	"bugs": "https://gitlab.rschluessler.com/raimund.schluessler/Inventory/issues",
	"contributors": [],
	"dependencies": {
		"@nextcloud/vue": "1.0.0",
		"axios": "0.19.0",
		"p-queue": "6.2.0",
		"papaparse": "5.1.0",
		"qr-image": "3.2.0",
		"search-query-parser": "1.5.3",
		"v-tooltip": "2.0.2",
		"vue": "2.6.10",
		"vue-click-outside": "1.0.7",
		"vue-qrcode-reader": "2.1.1",
		"vue-router": "3.1.3",
		"vuex": "3.1.1",
		"vuex-router-sync": "5.0.0"
	},
	"devDependencies": {
		"@babel/core": "7.6.4",
		"@babel/plugin-proposal-object-rest-spread": "7.6.2",
		"@babel/plugin-syntax-dynamic-import": "7.2.0",
		"@babel/preset-env": "7.6.3",
		"@vue/test-utils": "1.0.0-beta.29",
		"babel-core": "7.0.0-bridge.0",
		"babel-eslint": "10.0.3",
		"babel-jest": "24.9.0",
		"babel-loader": "8.0.6",
		"css-loader": "3.2.0",
		"eslint": "6.5.1",
		"eslint-config-standard": "14.1.0",
		"eslint-friendly-formatter": "4.0.1",
		"eslint-loader": "3.0.2",
		"eslint-plugin-import": "2.18.2",
		"eslint-plugin-node": "10.0.0",
		"eslint-plugin-promise": "4.2.1",
		"eslint-plugin-standard": "4.0.1",
		"eslint-plugin-vue": "5.2.3",
		"file-loader": "4.2.0",
		"jest": "24.9.0",
		"jest-serializer-vue": "2.0.2",
		"jsdom": "15.2.0",
		"jsdom-global": "3.0.2",
		"node-sass": "4.12.0",
		"prettier-eslint": "9.0.0",
		"raw-loader": "3.1.0",
		"regenerator-runtime": "0.13.3",
		"sass-loader": "8.0.0",
		"stylelint": "11.1.1",
		"stylelint-config-recommended-scss": "4.0.0",
		"stylelint-scss": "3.12.0",
		"stylelint-webpack-plugin": "1.0.2",
		"svg-sprite": "1.5.0",
		"terser-webpack-plugin": "2.2.1",
		"vue-jest": "3.0.5",
		"vue-loader": "15.7.1",
		"vue-template-compiler": "2.6.10",
		"webpack": "4.41.2",
		"webpack-cli": "3.3.9",
		"webpack-merge": "4.2.2"
	},
	"engines": {
		"node": ">=10.0.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"vue"
		],
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1"
		},
		"transform": {
			"^.+\\.js$": "<rootDir>/node_modules/babel-jest",
			".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
		},
		"snapshotSerializers": [
			"<rootDir>/node_modules/jest-serializer-vue"
		],
		"setupFilesAfterEnv": [
			"./tests/setup.js"
		],
		"collectCoverage": true,
		"collectCoverageFrom": [
			"<rootDir>/src/**/*.{js,vue}",
			"!**/node_modules/**",
			"!**/vendor/**"
		],
		"coverageReporters": [
			"json",
			"text",
			"html"
		]
	}
}
